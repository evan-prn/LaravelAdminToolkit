{
    "name": "evan-prn/laravel-admin-toolkit",
    "description": "Laravel Admin Toolkit - Pro Laravel package boilerplate",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "PERNOT Evan",
        "email": "pernot.evan03@gmail.com"
    }],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "spatie/laravel-permission": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "nunomaduro/collision": "^7.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/phpstan": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "AdminToolkit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AdminToolkit\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AdminToolkit\\AdminToolkitServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "pest",
        "analyse": "phpstan analyse",
        "format": "php-cs-fixer fix"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}